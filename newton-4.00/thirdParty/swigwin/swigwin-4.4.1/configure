    printf "%s\n" "$as_me:${as_lineno-$LINENO}: $ac_msg" >&5
    printf "%s\n" "$ac_msg" >&6
    as_dir="$ac_dir"; as_fn_mkdir_p
    ac_builddir=.

case "$ac_dir" in
.) ac_dir_suffix= ac_top_builddir_sub=. ac_top_build_prefix= ;;

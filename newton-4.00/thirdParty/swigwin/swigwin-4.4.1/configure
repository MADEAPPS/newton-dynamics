*-*-darwin*)
        C_LDFLAGS='-dynamiclib'
        C_SO=".dylib"
        ;;
*-*-cygwin* | *-*-mingw*)
        # Nothing special to do.
        ;;
*)
        # This is needed for linking with Boost which uses mutexes and does no
        # harm in all the other cases.
        C_LDFLAGS='-pthread'
        ;;
esac

C_LDSHARED="\$(LDSHARED) $C_LDFLAGS"
CXX_LDSHARED="\$(CXXSHARED) $C_LDFLAGS"





